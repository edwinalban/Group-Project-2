<section class="card">
    <header class="animal-name">{{name}}</header>
    <div class="card-body">
        <img src="/assets/images/{{filename}}" alt="{{description}}">
    </div>
    <button class="voting-btn" id="{{id}}">Vote for me!</button>
    <div id="vote{{id}}">
        {{voteCount}}
    </div>
</section>